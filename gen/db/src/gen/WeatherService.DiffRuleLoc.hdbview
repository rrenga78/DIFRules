VIEW WeatherService_DiffRuleLoc AS SELECT
  RULE_CONFIG_ASSIGNMENT_LOCATIONS_0.sKey,
  RULE_CONFIG_ASSIGNMENT_LOCATIONS_0.Client,
  RULE_CONFIG_ASSIGNMENT_LOCATIONS_0.DIFConfig,
  RULE_CONFIG_ASSIGNMENT_LOCATIONS_0.DDF_Location
FROM weatherdata_db_RULE_CONFIG_ASSIGNMENT_LOCATIONS AS RULE_CONFIG_ASSIGNMENT_LOCATIONS_0
WITH ASSOCIATIONS (
  MANY TO ONE JOIN WeatherService_DiffRuleConfig AS DIFConfigRef ON (DIFConfigRef.sKey = DIFConfig)
)