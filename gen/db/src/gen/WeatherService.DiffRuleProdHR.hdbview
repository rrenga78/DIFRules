VIEW WeatherService_DiffRuleProdHR AS SELECT
  RULE_CONFIG_ASSIGNMENT_PRODUCT_NODES_0.sKey,
  RULE_CONFIG_ASSIGNMENT_PRODUCT_NODES_0.Client,
  RULE_CONFIG_ASSIGNMENT_PRODUCT_NODES_0.DIFConfig,
  RULE_CONFIG_ASSIGNMENT_PRODUCT_NODES_0.DDF_Product_Hierarchy_Node
FROM weatherdata_db_RULE_CONFIG_ASSIGNMENT_PRODUCT_NODES AS RULE_CONFIG_ASSIGNMENT_PRODUCT_NODES_0
WITH ASSOCIATIONS (
  MANY TO ONE JOIN WeatherService_DiffRuleConfig AS DIFConfigRef ON (DIFConfigRef.sKey = DIFConfig)
)