COLUMN TABLE weatherdata_db_RULE_CONFIG_AGGREGATIONS (
  sKey NVARCHAR(36) NOT NULL,
  Client NVARCHAR(3) NOT NULL,
  DIFConfig NVARCHAR(36) NOT NULL,
  Measure NVARCHAR(36) NOT NULL,
  Function NVARCHAR(100) NOT NULL,
  PRIMARY KEY(sKey)
) WITH ASSOCIATIONS (
  MANY TO ONE JOIN weatherdata_db_RULE_CONFIG AS DIFConfigRef ON (DIFConfigRef.sKey = DIFConfig)
)